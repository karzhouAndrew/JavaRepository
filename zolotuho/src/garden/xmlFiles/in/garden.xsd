<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.it-academy.by"
           xmlns="http://www.it-academy.by"
           elementFormDefault="qualified">
    <xs:element name="plants">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="tree" type="treeType" minOccurs="0" maxOccurs="5000"/>
                <xs:element name="bush" type="bushType" minOccurs="0" maxOccurs="5000"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="bushType">
        <xs:sequence>
            <xs:element name="name" type="xs:string"/>
            <xs:element name="age" type="xs:integer"/>
            <xs:element name="height" type="xs:integer"/>
            <xs:element name="trunkQuantity" type="xs:integer"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="treeType">
        <xs:sequence>
            <xs:element name="name" type="xs:string"/>
            <xs:element name="age" type="xs:integer"/>
            <xs:element name="height" type="xs:integer"/>
            <xs:element name="trunkWidth" type="xs:integer"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>