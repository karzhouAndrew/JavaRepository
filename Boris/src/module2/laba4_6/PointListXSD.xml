<?xml version="1.0" encoding="UTF-8"?>
<PointList xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
           xs:noNamespaceSchemaLocation="PointList.xsd">
    <point units="cm">
        <x>2</x>
        <y>3</y>
    </point>
    <point units="mm">
        <x>9</x>
        <y>3</y>
    </point>
    <point units="px">
        <x>10</x>
        <y>0</y>
    </point>
</PointList>

